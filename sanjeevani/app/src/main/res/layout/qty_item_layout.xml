<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/textView231"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16.0dip"
        android:layout_marginTop="8.0dip"
        android:text="Quantity"
        android:textColor="#ff000000"
        android:textSize="18.0sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/qty_chnage_text_dia1"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:layout_marginStart="16.0dip"
        android:layout_marginTop="16.0dip"
        android:layout_marginEnd="16.0dip"
        android:backgroundTint="#ff000000"
        android:ems="10"
        android:hint="Qty"
        android:inputType="number"
        android:textColor="#ff000000"
        android:textSize="12.0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView231" />

    <LinearLayout
        android:id="@+id/linearLayout91"
        android:layout_width="0.0dip"
        android:layout_height="wrap_content"
        android:layout_marginTop="8.0dip"
        android:elevation="3.0dip"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/qty_chnage_text_dia1">

        <Button
            android:id="@+id/cancle_btn_dia1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#ffffffff"
            android:text="CANCLE"
            android:textColor="#ff000000" />

        <Button
            android:id="@+id/save_btn_dia1"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1.0"
            android:background="#ffffffff"
            android:text="SAVE"
            android:textColor="#ff000000" />
    </LinearLayout>

    <View
        android:id="@+id/divider121"
        android:layout_width="409.0dip"
        android:layout_height="1.0dip"
        android:background="@color/RecyclerViewBack"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/linearLayout91" />

    <View
        android:id="@+id/divider141"
        android:layout_width="1.0dip"
        android:layout_height="0.0dip"
        android:background="@color/RecyclerViewBack"
        app:layout_constraintBottom_toBottomOf="@id/linearLayout91"
        app:layout_constraintEnd_toEndOf="@id/linearLayout91"
        app:layout_constraintStart_toStartOf="@id/linearLayout91"
        app:layout_constraintTop_toBottomOf="@id/divider121" />
</androidx.constraintlayout.widget.ConstraintLayout>